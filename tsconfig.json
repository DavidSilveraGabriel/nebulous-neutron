{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  }
}